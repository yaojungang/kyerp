<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="Plate" extends="struts-ChengDe" namespace="/Plate">
		<action name="PlateAdmin" class="com.jzland.ChengDe.action.PlateAction"
			method="PlateAdmin">
			<result name="success">/Plate/PlateList.jsp</result>
		</action>
		<action name="PlateInfo" class="com.jzland.ChengDe.action.PlateAction"
			method="getPlateById">
			<result name="success">/Plate/PlateInfo.jsp</result>
		</action>
		<action name="PlateList" class="com.jzland.ChengDe.action.PlateAction"
			method="plateList">
			<result name="success">/Plate/PlateList.jsp</result>
		</action>
		<action name="addPlate" class="com.jzland.ChengDe.action.PlateAction"
			method="addPlate">
			<result name="success">/Plate/addPlate.jsp</result>
		</action>
		<action name="addPlate_save" class="com.jzland.ChengDe.action.PlateAction"
			method="addPlate_save">
			<result name="success">/Plate/PlateList.jsp</result>
		</action>
		<action name="ExpPlateList" class="com.jzland.ChengDe.action.PlateAction"
			method="getExpPlateList">
			<result name="success">/Plate/PlateList.jsp</result>
		</action>
		<action name="searchPlate" class="com.jzland.ChengDe.action.PlateAction"
			method="searchPlate">
			<result name="success">/Plate/PlateList.jsp</result>
		</action>
		<action name="delPlate" class="com.jzland.ChengDe.action.PlateAction"
			method="delPlate">
			<result name="success">/Plate/PlateList.jsp</result>
		</action>
		<action name="editPlate" class="com.jzland.ChengDe.action.PlateAction"
			method="editPlate">
			<result name="success">/Plate/PlateInput.jsp</result>
		</action>
		<action name="editPlate_save" class="com.jzland.ChengDe.action.PlateAction"
			method="editPlate_save">
			<result name="success">/Plate/PlateList.jsp</result>
		</action>
	</package>
</struts>
