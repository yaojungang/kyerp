<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tyopf.vo.BindingFactory" table="BindingFactory">
		<comment></comment>
		<id name="bfId" type="long">
			<column name="BF_ID" />
			<generator class="native" />
		</id>
		<property name="factory" type="string">
			<column name="Factory" length="50">
			</column>
		</property>
		<property name="linkman" type="string">
			<column name="Linkman" length="50">
			</column>
		</property>
		<property name="tel" type="string">
			<column name="Tel" length="50">
			</column>
		</property>
		<property name="technics" type="string">
			<column name="Technics" length="50">
			</column>
		</property>
		<property name="remark" type="string">
			<column name="Remark" length="50">
			</column>
		</property>
	</class>
</hibernate-mapping>
