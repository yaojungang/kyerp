<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/skin/newBlue">
		<excludes>
		<!-- 不应用模板的文件 -->
		<pattern>/debug.jsp</pattern>
		<pattern>/styles/*</pattern>
		<pattern>/scripts/*</pattern>
		<pattern>/images/*</pattern>
		<pattern>/dojo/*</pattern>
		<pattern>/struts/*</pattern>
		<pattern>/ajax/AjaxResult*</pattern>
		<pattern>/AjaxTest.action</pattern>
		<pattern>/ajax/remoteforms/AjaxRemoteForm.action</pattern>
		<pattern>/tags/ui/ajax/*</pattern>
		<pattern>/test/*</pattern>
		<pattern>/Library/ext/*</pattern>
		<pattern>/Library/js/*</pattern>
		<pattern>/*/*_Complete*</pattern>
		<pattern>/*/*getAllPresswork*</pattern>
		<pattern>/*/noSkin_*</pattern>
		<pattern>/*/*_noSkin*</pattern>
		<pattern>/Admin/*</pattern>
	</excludes>
	<decorator name="print" page="printable.jsp">
		<pattern>/*/*_print*</pattern>
	</decorator>
	<decorator name="main" page="main.jsp">
		<pattern>/*</pattern>
		<pattern>/*/AFInfo*</pattern>
		<pattern>/AF/getAFByNo*</pattern>
		<pattern>/*/ZK*</pattern>
		<pattern>/*/addAF_view*</pattern>
		<pattern>/*/saveAF*</pattern>
		<pattern>/*/editAF*</pattern>
		<pattern>/*/addAF_Element_view*</pattern>
	</decorator>
	<decorator name="pages" page="pages.jsp" />
	<decorator name="panel" page="panel.jsp" />
	<decorator name="printable" page="printable.jsp" />
</decorators>