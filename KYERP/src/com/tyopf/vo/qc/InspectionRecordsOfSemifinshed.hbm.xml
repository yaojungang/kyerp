<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tyopf.vo.qc.InspectionRecordsOfSemifinshed"
		table="inspectionRecordsOfSemifinshed">
		<id name="id" type="long">
			<column name="id" />
			<generator class="native" />
		</id>
		<many-to-one name="afBase" class="com.tyopf.vo.AfBase"
			column="af_id" lazy="false">
		</many-to-one>
		<property name="examdate" type="timestamp"></property>
		<property name="item" type="string">
			<column name="item" length="50"></column>
		</property>
		<list name="operatorsEmployees" table="Employee" cascade="persist">
			<key column="inspectionRecordsOfSemifinshed_id" />
			<list-index column="inspectionRecordsOfSemifinshed_index" />
			<one-to-many class="com.tyopf.vo.Employee" />
		</list>
		<property name="remark" type="string">
			<column name="remark" length="200"></column>
		</property>
	</class>
</hibernate-mapping>
